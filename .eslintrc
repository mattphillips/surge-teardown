{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true
  },
  "globals": {
    "Promise": true
  },
  "plugins": [],
  "extends": "eslint:recommended",
  "rules": {
    "comma-spacing": [
      1,
      {
        "before": false,
        "after": true
      }
    ],
    "eol-last": 1,
    "indent": [
      "warn",
      2
    ],
    "key-spacing": 1,
    "no-multi-spaces": 1,
    "keyword-spacing": 1,
    "no-unused-vars": 1,
    "no-trailing-spaces": 1,
    "object-curly-spacing": [
      1,
      "always"
    ],
    "quotes": [
      "error",
      "single",
      {
        "avoidEscape": true
      }
    ],
    "semi": 1,
    "no-console": "off"
  }
}
